{
  "name": "api-boilerplate",
  "description": "Simple Node.js+Express+Sequelize API server.js boilerplate",
  "version": "1.0.0",
  "author": "Diogo SimÃµes",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://simoes12@bitbucket.org/simoes12/api-repository.git"
  },
  "keywords": [
    "express",
    "sequelize",
    "boilerplate",
    "rest",
    "restful",
    "api"
  ],
  "scripts": {
    "dev": "NODE_ENV=development DEBUG=* sequelize db:migrate && nodemon ./src/bin/server.js",
    "dev-hard": "NODE_ENV=development sequelize db:drop && sequelize db:create && sequelize db:migrate && DEBUG=* nodemon ./src/bin/server.js",
    "pretest": "NODE_ENV=test sequelize db:migrate",
    "test": "NODE_ENV=development mocha -r chai/register-should --recursive --exit",
    "start": "node ./src/bin/server.js",
    "coverage": "NODE_ENV=test nyc --reporter=html mocha -r chai/register-should --recursive --exit",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "coverage-rep": "NODE_ENV=test nyc --reporter=text-lcov > coverage.lcov mocha -r chai/register-should --recursive --exit && codecov"
  },
  "dependencies": {
    "app-root-path": "2.1.0",
    "bcrypt": "3.0.2",
    "bcrypt-promise": "2.0.0",
    "compression": "1.7.3",
    "cors": "2.8.5",
    "debug": "4.1.0",
    "express": "4.16.4",
    "express-validator": "5.3.0",
    "helmet": "3.15.0",
    "http-errors": "1.7.1",
    "http-status-codes": "1.3.0",
    "jsonwebtoken": "8.4.0",
    "lodash": "4.17.11",
    "morgan": "1.9.1",
    "nodemailer": "4.6.8",
    "passport": "0.4.0",
    "passport-jwt": "4.0.0",
    "pg": "7.6.1",
    "sequelize": "4.41.2",
    "sparkpost": "2.1.3"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "chai-http": "4.2.0",
    "codecov": "3.1.0",
    "coveralls": "3.0.2",
    "dotenv": "6.1.0",
    "mocha": "5.2.0",
    "nodemon": "1.18.6",
    "nyc": "13.1.0",
    "sequelize-cli": "5.3.0",
    "sinon": "7.1.1"
  },
  "engines": {
    "node": ">= 8.11.3"
  }
}
