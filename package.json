    "http-status-codes": "1.3.0",
    "lodash": "4.17.10",
    "morgan": "1.9.0",
    "pg": "7.4.3",
    "pg-hstore": "2.3.2",
    "sequelize": "4.38.0",
    "sequelize-cli": "4.0.0",
    "throw.js": "^3.0.1"
