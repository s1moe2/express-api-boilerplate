{
  "name": "api-boilerplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development sequelize db:migrate && DEBUG=app:servers nodemon ./src/bin/server.js",
    "pretest": "NODE_ENV=test sequelize db:migrate",
    "test": "NODE_ENV=test mocha -r chai/register-should --recursive --exit",
    "start": "node ./src/bin/server.js",
    "coverage": "NODE_ENV=test nyc --reporter=html mocha ./test/end-to-end.js -r chai/register-should --exit",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "coverage-rep": "NODE_ENV=test nyc --reporter=text-lcov > coverage.lcov mocha ./test/end-to-end.js -r chai/register-should --exit && codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://simoes12@bitbucket.org/simoes12/api-repository.git"
  },
  "dependencies": {
    "app-root-path": "2.1.0",
    "await-to-js": "2.0.1",
    "bcrypt": "3.0.0",
    "bcrypt-promise": "2.0.0",
    "compression": "1.7.3",
    "cors": "2.8.4",
    "debug": "2.6.9",
    "express": "4.16.0",
    "express-validator": "5.2.0",
    "helmet": "3.12.1",
    "http-errors": "1.6.2",
    "http-status-codes": "1.3.0",
    "jsonwebtoken": "8.3.0",
    "lodash": "4.17.10",
    "morgan": "1.9.0",
    "passport": "0.4.0",
    "pg": "7.4.3",
    "pg-hstore": "2.3.2",
    "sequelize": "4.38.0"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "chai-http": "4.0.0",
    "codecov": "^3.0.4",
    "coveralls": "^3.0.2",
    "mocha": "5.2.0",
    "nodemon": "1.17.5",
    "nyc": "^12.0.2",
    "sequelize-cli": "^4.0.0",
    "sinon": "^6.1.3"
  }
}
